# Streaming Middleware Environment Variables
# Copy this file to .env and fill in your values

# Stream Mode: "camera" (default) or "test"
STREAM_MODE=camera

# Camera Configuration (used when STREAM_MODE=camera)
CAMERA_IP=your_camera_ip
RTSP_PORT=554
STREAM_LOW_PATH=stream2
STREAM_HIGH_PATH=stream1
GATE_ID=gate1

# Test Video Configuration (used when STREAM_MODE=test)
TEST_VIDEO_PATH=/videos/test_video.mp4

# Host volume path for test videos
TEST_VIDEOS_HOST_PATH=/path/to/your/videos
